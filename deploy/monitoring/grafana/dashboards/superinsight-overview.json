{
  "dashboard": {
    "id": null,
    "title": "SuperInsight Platform Overview",
    "tags": ["superinsight", "overview"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "superinsight_system_cpu_usage_percent",
            "legendFormat": "CPU Usage %"
          },
          {
            "expr": "superinsight_system_memory_usage_percent",
            "legendFormat": "Memory Usage %"
          },
          {
            "expr": "superinsight_system_disk_usage_percent",
            "legendFormat": "Disk Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "superinsight_active_users_count",
            "legendFormat": "Active Users"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "HTTP Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(superinsight_http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "HTTP Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(superinsight_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(superinsight_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Annotation Efficiency",
        "type": "graph",
        "targets": [
          {
            "expr": "superinsight_annotation_efficiency_per_hour",
            "legendFormat": "Annotations/Hour"
          }
        ],
        "yAxes": [
          {
            "label": "Annotations/Hour",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Annotation Quality Score",
        "type": "graph",
        "targets": [
          {
            "expr": "superinsight_annotation_quality_score",
            "legendFormat": "Quality Score"
          }
        ],
        "yAxes": [
          {
            "label": "Score",
            "min": 0,
            "max": 1
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 7,
        "title": "AI Model Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(superinsight_ai_inferences_total[5m])",
            "legendFormat": "{{model}} inferences/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Inferences/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 8,
        "title": "AI Confidence Scores",
        "type": "graph",
        "targets": [
          {
            "expr": "superinsight_ai_confidence_score",
            "legendFormat": "{{model}} confidence"
          }
        ],
        "yAxes": [
          {
            "label": "Confidence",
            "min": 0,
            "max": 1
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 9,
        "title": "Database Query Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(superinsight_database_queries_total[5m])",
            "legendFormat": "{{type}} queries/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Queries/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32}
      },
      {
        "id": 10,
        "title": "Project Completion Status",
        "type": "graph",
        "targets": [
          {
            "expr": "superinsight_project_completion_percentage",
            "legendFormat": "{{project_id}} completion %"
          }
        ],
        "yAxes": [
          {
            "label": "Completion %",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32}
      }
    ]
  }
}